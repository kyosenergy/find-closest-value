{
    "name": "kyos/find-closest-value",
    "description": "Finds the closest arithmetic match in an array, using Binary Search",
    "keywords": [
        "kyos",
        "find-closest-value"
    ],
    "homepage": "https://github.com/kyosenergy/find-closest-value",
    "license": "MIT",
    "authors": [
        {
            "name": "Zois Pagoulatos",
            "email": "pagoulatos@kyos.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Kyos\\FindClosestValue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kyos\\FindClosestValue\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
